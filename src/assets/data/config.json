{
  "version": "0.5.0",
  "mapLayers": {
    "baseLayers": [
      {
        "name": "ESRI World Imagery",
        "url": "http://server.arcgisonline.com/ArcGIS/rest/services/World_Imagery/MapServer/tile/{z}/{y}/{x}",
        "type": "tile",
        "visible": false
      },
      {
        "name": "National Geographic",
        "layer": "NationalGeographic",
        "type": "agsbase",
        "visible": true
      },
      {
        "name": "Open Street Map",
        "url": "https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png",
        "layerOptions": {
          "maxZoom": 18,
          "attribution": "..."
        },
        "type": "tile",
        "visible": false
      },
      {
        "name": "Streets",
        "layer": "Streets",
        "type": "agsbase",
        "visible": false
      },
      {
        "name": "Topographic",
        "layer": "Topographic",
        "type": "agsbase",
        "visible": false
      }
    ],
    "overlayLayers": [
      {
        "name": "USGSHydroCached",
        "url": "https://basemap.nationalmap.gov/arcgis/rest/services/USGSHydroCached/MapServer",
        "type": "agsTile",
        "layerOptions": {
          "opacity": 0.6,
          "zIndex": 9999,
          "useCors": false
        },
        "visible": true
      }
    ],
    "markerOptions": {
      "Spill": {
        "iconUrl": "https://cdn.rawgit.com/pointhi/leaflet-color-markers/master/img/marker-icon-2x-black.png",
        "shadowUrl": "https://cdnjs.cloudflare.com/ajax/libs/leaflet/0.7.7/images/marker-shadow.png",
        "iconSize": [ 25, 41 ],
        "iconAnchor": [ 12, 41 ],
        "popupAnchor": [ 1, -34 ],
        "shadowSize": [ 41, 41 ]
      },
      "Uptake": {
        "iconUrl": "https://cdn.rawgit.com/pointhi/leaflet-color-markers/master/img/marker-icon-2x-red.png",
        "shadowUrl": "https://cdnjs.cloudflare.com/ajax/libs/leaflet/0.7.7/images/marker-shadow.png",
        "iconSize": [ 25, 41 ],
        "iconAnchor": [ 12, 41 ],
        "popupAnchor": [ 1, -34 ],
        "shadowSize": [ 41, 41 ]
      },
      "EndNode": {
        "color": "orange",
        "fillColor": "#f03",
        "fillOpacity": 0.5,
        "radius": 100
      },
      "Reach": {
        "color": "orange",
        "fillColor": "#f03",
        "fillOpacity": 0.5,
        "radius": 100
      },
      "Polyline": {
        "color": "#0BDCD5",
        "weight": 3,
        "opacity": 1
      },
      "Polyline7": {
        "color": "#669900",
        "weight": 3,
        "opacity": 1
      },
      "Polyline6": {
        "color": "#CCFF00",  
