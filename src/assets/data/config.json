        "weight": 3,
        "opacity": 1
      },
      "Polyline4": {
        "color": "#FF6600", 
        "weight": 3,
        "opacity": 1
      },
      "Polyline3": {
        "color": "#FF0000",
        "weight": 3,
        "opacity": 1
      },
      "Polyline2": {
        "color": "#CC0000",
        "weight": 3,
        "opacity": 1
      },  
      "Polyline_upstream": {
        "color": "green",
        "weight": 3,
        "opacity": 1
      },
      "Polyline_break": {
        "color": "gray",
        "weight": 3,
        "opacity": 1
